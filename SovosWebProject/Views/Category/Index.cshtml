
@model IEnumerable<Category>
@{
    ViewData["Title"] = "Categories";
}

<h1>Index Category</h1>


<table class="table table-bordered table-striped w-100">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach(var obj in Model)
            {
                <tr>
                    <td width="50%">
                        @obj.Name
                    </td>
                    <td width="30%">
                        @obj.DisplayOrder
                    </td>
                </tr>
            }
        }
    </tbody>
</table>


